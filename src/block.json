{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "unbox/copyright",
	"version": "1.3.11",
	"title": "Copyright Line",
	"category": "design",
	"icon": "update",
	"description": "WordPress Block to set up a copyright line with auto updating year and Site Title pulled from Settings -> General",
	"keywords": [
		"footer",
		"current year"
	],
	"supports": {
		"html": true,
		"color": {
			"text": true,
            "background": true,
            "link": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"align": true,
		"customClassName": true,
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true,
			"fontFamily": true,
			"fontWeight": true,
			"fontStyle": true,
			"letterSpacing": true,
			"textTransform": true,
			"textDecoration": true
		}
	},
	"attributes": {
		"input": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	},
	"textdomain": "unbox",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
