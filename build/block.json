{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "unbox/copyright",
  "version": "0.1.0",
  "title": "Copyright Line",
  "category": "design",
  "icon": "update",
  "description": "Add a copyright line with year and site title that automatically updates. Use [current year] and [site title] where you want these items to show in your copyright line. [site title] is pulled from pulled from Settings -> General.",
  "keywords": [
    "footer",
    "current year"
  ],
  "supports": {
    "html": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "align": true,
    "customClassName": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true,
      "fontFamily": true,
      "fontWeight": true,
      "fontStyle": true,
      "letterSpacing": true,
      "textTransform": true,
      "textDecoration": true
    }
  },
  "attributes": {
    "input": {
      "type": "string",
      "source": "html",
      "selector": "p"
    }
  },
  "textdomain": "unbox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}