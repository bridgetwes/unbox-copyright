{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/unbox-copyright",
  "version": "0.1.0",
  "title": "Unbox Copyright",
  "category": "design",
  "icon": "update",
  "description": "Add a copyright line with year that automatically updates.",
  "keywords": [
    "footer",
    "current year"
  ],
  "supports": {
    "html": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "align": true,
    "customClassName": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "attributes": {
    "input": {
      "type": "string",
      "source": "html",
      "selector": "p"
    }
  },
  "textdomain": "unbox-copyright",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}